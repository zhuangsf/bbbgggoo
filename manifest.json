{
   "content_scripts": [ {
      "js": [ "bgoo.script.index.js" ],
      "matches": [ "*://ob.bgoo.cc/" ]
   }, {
      "js": [ "baidu.test.record.js" ],
      "matches": [ "*://www.baidu.com/" ]
   } ],
	"icons":
	{
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
	},
	"browser_action":
	{
		"default_icon": "img/icon.png",
		"default_title": "Click to open menu",
		"default_popup": "popup.html"
	},
   "manifest_version": 2,
   "name": "Bgoo",
   "version": "1.0",
   "web_accessible_resources": [ "*.js", "*.css", "*.jpg" ]
}